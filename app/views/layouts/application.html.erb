<!DOCTYPE html>
<html>
<head>
  <title>JW Seguros</title>
  <link rel="icon" href="/assets/jw_icon.ico">
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
	<body>
		<%= render 'helpers/header' %>
		<%= render 'helpers/navbar' %>

		<div class="container">
			<div class="row">
				<div class="col-lg-6">
						<% if flash[:alert] %>
					<div class="alert alert-info" role="alert">
					  	<p class="flash-error"><%= flash[:alert] %></p>
					</div>
					 	<% end %>
					 	<% if flash[:notice] %>
					<div class="alert alert-info" role="alert">
					  	<p class="flash-notice"><%= flash[:notice] %></p>
					</div>
					 	<% end %>
				</div>
			</div>
		</div>
	<%= yield %>

		
		<%= render 'helpers/footer' %>
	<%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
	</body>
</html>
